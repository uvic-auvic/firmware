{
    "configurations": [
        {
            "name": "powerboard2",
            "includePath": [
                "${workspaceFolder}/firmware/shared/code/**",
                "${workspaceFolder}/firmware/shared/data/**",
                "${workspaceFolder}/firmware/powerboard2/code/**",
                "${workspaceFolder}/firmware/libs/STM/STM32F4xx_DSP_StdPeriph_Lib_V1.8.0/Libraries/STM32F4xx_StdPeriph_Driver/inc/*",
                "${workspaceFolder}/firmware/libs/STM/STM32F4xx_DSP_StdPeriph_Lib_V1.8.0/Libraries/CMSIS/Include/*",
                "${workspaceFolder}/firmware/libs/STM/STM32F4xx_DSP_StdPeriph_Lib_V1.8.0/Libraries/CMSIS/Device/ST/STM32F4xx/Include/*",
                "${workspaceFolder}/firmware/libs/FreeRTOS/FreeRTOS/Source/include/*",
                "${workspaceFolder}/firmware/libs/FreeRTOS/FreeRTOS/Source/portable/GCC/ARM_CM4F/*"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "defines": [
                "UART_DEBUG",
                "STM32",
                "STM32F4",
                "STM32F413_423xx",
                "DEVICE_STM32F413=1",
                "MCU_ARCHITECTURE_M4=1",
                "DEVICE_FAMILY_STM32F4=1",
                "USE_STDPERIPH_DRIVER"
            ]
        }
    ],
    "version": 4
}
